
&i2c0 {
	compatible = "nordic,nrf-twi";
	status = "okay";
	hts221@5f {
		compatible = "st,hts221";
		reg = <0x5f>;
	};
	lis3dh@18 {
		compatible = "st,lis3dh", "st,lis2dh";
		reg = <0x18>;
		irq-gpios = <&gpio0 12 GPIO_ACTIVE_HIGH>;
	};
	lps25hb@5c {
		compatible = "st,lps25hb-press";
		reg = <0x5c>;
	};
};